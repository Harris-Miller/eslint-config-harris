{
  "name": "eslint-config-harris",
  "version": "2.0.0-beta.7",
  "description": "Harris' ESLint config",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Harris-Miller/node-eslint-config-harris.git"
  },
  "scripts": {
    "bump": "bump --prompt --commit --tag",
    "bump:rc": "npm run bump -- --preid=rc",
    "bump:beta": "npm run bump -- --preid=beta",
    "lint": "eslint **/*.js --ignore-pattern=tests",
    "test": "eslint tests/**/*.js tests/**/*.jsx"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "typescript",
    "tslint",
    "harris"
  ],
  "author": "Harris Miller <harrismillerconsulting@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^10.0.1",
    "eslint": "5.15.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.4.1",
    "version-bump-prompt": "^4.1.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": ">= 10.0.1",
    "eslint": ">= 5.15.1",
    "eslint-plugin-babel": ">= 5.3.0",
    "eslint-plugin-import": ">= 2.16.0",
    "eslint-plugin-jsx-a11y": ">= 6.0.3",
    "eslint-plugin-react": ">= 7.9.1"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.6"
  }
}
