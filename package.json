{
  "name": "eslint-config-harris",
  "version": "1.6.0",
  "description": "Harris' ESLint config",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Harris-Miller/node-eslint-config-harris.git"
  },
  "scripts": {
    "bump": "npm run lint && npm run test && bump --prompt --commit --tag",
    "bump:rc": "npm run bump -- --preid=rc",
    "lint": "eslint **/*.js --ignore-pattern=tests",
    "test": "eslint tests/**/*.js tests/**/*.jsx"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "harris"
  ],
  "author": "Harris Miller <harrismillerconsulting@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "react": "^16.4.1",
    "version-bump-prompt": "^4.1.0"
  },
  "peerDependencies": {
    "babel-eslint": ">= 8.2.3",
    "eslint": ">= 4.19.1",
    "eslint-plugin-babel": ">= 5.1.0",
    "eslint-plugin-filenames": ">= 1.3.2",
    "eslint-plugin-import": ">= 2.12.0",
    "eslint-plugin-jsx-a11y": ">= 6.0.3",
    "eslint-plugin-react": ">= 7.9.1"
  }
}