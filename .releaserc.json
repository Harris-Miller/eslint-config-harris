{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    "next-major",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "config": "conventional-changelog-ayx",
        "releaseRules": "conventional-changelog-ayx/release-rules"
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "config": "conventional-changelog-ayx"
      }
    ]
  ]
}
